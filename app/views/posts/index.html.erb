
  <fieldset>
    <legend>Post overview</legend>
  </fieldset>
    <% Post.all.each do |post| %>
      <div class="panel panel-default" style="height: 300px;">
        <div class ="panel-body">
          <div class="col-md-12">
            <div class="post_text">
              <h2><%= post.title %></h2>
              <%= post.content %>
            </div>
          </div>
        </div>

      </div>
    <% end %>


<br>


<%= link_to 'New Post', new_post_path %>
